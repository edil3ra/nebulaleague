{
    "name": "Starfighter",
    "version": "1.0.2",
    "description": "Starfighter",
    "main": "src/client/index.ts",
    "engines": {
        "node": "12.18.3"
    },
    "scripts": {
        "clean": "rimraf public/css public/*.js rimraf public/css public/*.txt index.html",
        "dev": "env NODE_ENV=development tsc-watch --onSuccess \"node src/server/server.js\"",
        "dev:client": "webpack-dev-server --config client/webpack/dev.config.js",
        "build:client": "webpack --config webpack.prod.config.js",
        "build:server": "tsc -p .",
        "prod": "env NODE_ENV=production npm-run-all client:prod server:prod",
        "predeploy": "npm run clean && npm install",
        "deploy": "env NODE_ENV=production npm-run-all build:client build:server",
        "postdeploy": "node src/server/server.js",
        "start": "npm run dev"
    },
    "author": "edil3ra",
    "license": "MIT",
    "dependencies": {
        "express": "^4.17.1",
        "fluent-behavior-tree": "^1.2.6",
        "html-webpack-plugin": "^4.0.0-beta.14",
        "phaser": "^3.24.1",
        "socket.io": "^2.3.0",
        "socket.io-client": "^2.3.0",
        "ts-behavior-tree": "^1.2.0",
        "uuid": "^8.2.0",
        "dotenv-flow": "^3.2.0",
        "bulma": "^0.9.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.7",
        "@types/node": "^14.0.27",
        "@types/socket.io": "^2.1.10",
        "@types/socket.io-client": "^1.4.33",
        "@typescript-eslint/eslint-plugin": "^3.6.1",
        "@typescript-eslint/parser": "^3.6.1",
        "css-loader": "^4.2.0",
        "eslint": "^7.4.0",
        "eslint-loader": "^4.0.2",
        "expose-loader": "0.7.5",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.14.1",
        "nodemon": "^2.0.4",
        "npm-run-all": "^4.1.5",
        "resolve": "^1.17.0",
        "rimraff": "0.0.1-security",
        "sass-loader": "^9.0.2",
        "style-loader": "^1.2.1",
        "terser-webpack-plugin": "^3.0.7",
        "ts-loader": "^8.0.1",
        "ts-node": "^8.10.2",
        "tsc-watch": "^4.2.9",
        "tslint": "^6.1.2",
        "typescript": "^3.9.6",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-dev-middleware": "^3.7.2",
        "webpack-dev-server": "^3.1.14",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-livereload-plugin": "^2.3.0"
    }
}
