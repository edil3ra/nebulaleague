{
    "name": "Nebulaleague",
    "version": "1.0.2",
    "description": "Nebulaleague",
    "engines": {
        "node": "12.18.3"
    },
    "scripts": {
        "clean": "rimraf public/css public/*.js rimraf public/css public/*.txt index.html",
        "dev": "env NODE_ENV=development tsc-watch --onSuccess \"node dist/server/server.js\"",
        "dev:client": "webpack-dev-server --config client/webpack/dev.config.js",
        "build:client": "webpack --config webpack.prod.config.js",
        "build:server": "tsc",
        "prod": "env NODE_ENV=production npm-run-all client:prod server:prod",
        "predeploy": "npm run clean && npm install",
        "deploy": "env NODE_ENV=production npm-run-all build:client build:server",
        "postdeploy": "node dist/server/server.js",
        "start": "npm run dev",
        "lint": "eslint src/**/**/*",
        "lint:server": "eslint src/server/**/*",
        "lint:shared": "eslint src/shared/**/*",
        "lint:client": "eslint src/client/**/*"
    },
    "author": "edil3ra",
    "license": "MIT",
    "_moduleAliases": {
        "~": "dist/."
    },
    "dependencies": {
        "@colyseus/monitor": "^0.12.2",
        "@colyseus/social": "^0.10.9",
        "@types/matter-js": "^0.14.8",
        "bulma": "^0.9.1",
        "colyseus": "^0.14.4",
        "colyseus.js": "^0.14.1",
        "cors": "^2.8.5",
        "deep-object-diff": "^1.1.0",
        "dotenv-flow": "^3.2.0",
        "express": "^4.17.1",
        "fluent-behavior-tree": "^1.2.6",
        "html-webpack-plugin": "^4.5.0",
        "install": "^0.13.0",
        "matter-js": "^0.14.2",
        "module-alias": "^2.2.2",
        "npm": "^6.14.10",
        "object-assign-deep": "^0.4.0",
        "phaser": "^3.50.0",
        "phaser-matter-collision-plugin": "^0.10.2",
        "stats.js": "^0.17.0",
        "ts-behavior-tree": "^1.2.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@colyseus/loadtest": "^0.14.2",
        "@types/cors": "^2.8.9",
        "@types/dotenv-flow": "^3.1.0",
        "@types/express": "^4.17.9",
        "@types/lodash": "^4.14.165",
        "@types/node": "^14.14.14",
        "@types/object-assign-deep": "^0.4.0",
        "@types/stats.js": "^0.17.0",
        "@types/uuid": "^8.3.0",
        "@types/webpack": "^4.41.25",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "css-loader": "^4.3.0",
        "eslint": "^6.6.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-import": "^2.22.1",
        "expose-loader": "0.7.5",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.14.1",
        "nodemon": "^2.0.6",
        "npm-run-all": "^4.1.5",
        "resolve": "^1.19.0",
        "rimraff": "0.0.1-security",
        "sass-loader": "^9.0.3",
        "style-loader": "^1.3.0",
        "terser-webpack-plugin": "^3.1.0",
        "ts-loader": "^8.0.12",
        "ts-node": "^8.10.2",
        "tsc-watch": "^4.2.9",
        "tslint": "^6.1.3",
        "typescript": "^3.9.7",
        "webpack": "^4.44.2",
        "webpack-bundle-analyzer": "^3.9.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-middleware": "^3.7.3",
        "webpack-dev-server": "^3.11.0",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-livereload-plugin": "^2.3.0"
    }
}
